<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="470.0" prefWidth="641.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.moesearchsearch.MoeHomeController">
   <children>
      <VBox>
         <children>
            <VBox>
               <children>
                  <VBox spacing="5.0">
                     <children>
                        <HBox>
                           <children>
                              <Label text="Moe Screen Search" />
                           </children>
                           <padding>
                              <Insets left="2.0" top="2.0" />
                           </padding>
                        </HBox>
                        <HBox spacing="5.0">
                           <children>
                              <TextField fx:id="URLTextField" promptText="URL...." />
                              <Button mnemonicParsing="false" onAction="#onClick_Search" text="Search" />
                              <Label fx:id="similarityLabel" prefHeight="17.0" prefWidth="305.0" text="Label">
                                 <HBox.margin>
                                    <Insets left="30.0" />
                                 </HBox.margin>
                              </Label>
                           </children>
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </HBox>
                     </children>
                     <padding>
                        <Insets left="2.0" top="2.0" />
                     </padding>
                  </VBox>
                  <HBox prefHeight="218.0" prefWidth="611.0" spacing="40.0">
                     <children>
                        <VBox>
                           <children>
                              <Label text="Results" />
                              <ListView fx:id="listView" prefHeight="201.0" prefWidth="273.0" />
                           </children>
                        </VBox>
                        <VBox />
                        <ImageView fx:id="imageView" fitHeight="216.0" fitWidth="312.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                     <VBox.margin>
                        <Insets left="30.0" top="40.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
            <HBox prefHeight="100.0" prefWidth="607.0">
               <children>
                  <Label fx:id="detailsLable" text="Label" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
